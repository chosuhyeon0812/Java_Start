package challenge;

import java.util.Scanner;

import day02.gugudan.Gugudan3;

public class gugudanMain {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int num = sc.nextInt();
		for (int i = 0; i < num; i++) {
			int[] result = Gugudan.calculate(i);
			Gugudan.print(result);
		}
	}
}
